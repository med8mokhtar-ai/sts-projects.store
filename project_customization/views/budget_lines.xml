<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data>

        <record id="view_crossovered_budget_line_tree" model="ir.ui.view">
            <field name="name">crossovered.budget.lines.tree</field>
            <field name="model">crossovered.budget.lines</field>
            <field name="inherit_id" ref="account_budget_oca.view_crossovered_budget_line_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='practical_amount']" position="after">
                    <field name="budget_credit"/>
                    <field name="budget_debit"/>
                </xpath>
                
                

            </field>
        </record>


    </data>
</odoo>